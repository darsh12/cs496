{% extends 'homepage.html.twig' %}
{% trans_default_domain 'FOSUserBundle' %}

{% block profile_tab %}
    <li class="tab" onclick="toggleCurrentTab(this)"><a class="current" href="{{ path('fos_user_profile_show') }}">Profile<span class="sr-only"></span></a></li>
{% endblock %}

{% block profile %}
<div class="fos_user_user_show">
    <p>{{ 'profile.show.username'|trans }}: {{ user.username }}</p>
    <p>{{ 'profile.show.email'|trans }}: {{ user.email }}</p>
</div>
{% endblock %}